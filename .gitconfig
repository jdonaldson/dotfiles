[core]
	excludesfile = ~/.gitignore
	quotepath = false
	autocrlf = input
	editor=nvim
	pager = less -R
[user]
	name = Justin Donaldson
	email = jdonaldson@gmail.com
[github]
  user = jdonaldson
[url "ssh://git@github.com/"]
 	insteadOf = https://github.com/
[includeIf "gitdir:~/Projects/com/"]
  path = ~/Projects/com/.gitconfig
[alias]
  nit = "!f() { git commit -m \"$@\" && git push; }; f"
  co = checkout
  br = branch
  ci = commit
  s = status
  u =  "!f() { git add -u && git status; }; f"
  default = current
[pull]
	rebase = true
[push]
	default = current
